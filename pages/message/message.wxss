.menu-bar{
    display: flex;
    width:100%;
}
.menu-bar>navigator{
    flex:1;
    padding:30rpx;
    text-align: center;  
}
.menu-bar>navigator image{
    width: 90rpx;
    height: 90rpx;
    padding: 20rpx auto;
}
.menu-bar view.menu{
    padding: 10rpx 0;
}
swiper{
    height: 280rpx;
}
.slide-image{
    width:100%;
    height:280rpx;
}
.messages{
    background: rgba(242,242,242,0.7);
    padding: 20rpx 0;
}
.message{
    background: #fff;
}
.message-info{
    padding: 30rpx;
    overflow: hidden;
}
.message-info image{
    width: 80rpx;
    height: 80rpx;
    float: left;
}
.user-id,.time{
    margin-left:100rpx;
    padding:5rpx;
}
.time{
    font-size: 25rpx;
    color:#666;
}
.message .title{
    margin: 0 30rpx;
    padding: 30rpx;
    font-size: 36rpx;
}